<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sheeta1998.mapper.EmpExprMapper">

    <insert id="insertBatch">
        insert into emp_expr(emp_id, begin, end, company, job)
        values
        <foreach collection="empExprs" item="empExpr" separator=",">
       (#{empExpr.empId},#{empExpr.begin},#{empExpr.end},#{empExpr.company},#{empExpr.job})
        </foreach>
    </insert>


    <delete id="deleteBatch">
        delete from emp_expr where emp_id in
                        <foreach collection="ids" separator="," open="(" close=")" item="empid">
                        #{empid}
                        </foreach>
    </delete>
</mapper>